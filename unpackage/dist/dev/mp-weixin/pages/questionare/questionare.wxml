<view class="u-p-30"><view class="u-m-b-20 u-font-28 text-black text-bold">{{''+title+desc+''}}</view><block wx:for="{{questionList}}" wx:for-item="item" wx:for-index="index" wx:key="id"><block><view class=" u-m-t-20"><view class="title u-font-28 text-black">{{''+(index+1)+" "+item.title+''}}</view><block wx:if="{{item.type===0}}"><u-radio-group vue-id="{{'e4b65cbc-1-'+index}}" wrap="{{true}}" active-color="#DC2672" data-event-opts="{{[['^change',[['radioGroupChange',['$event','$0'],[[['questionList','id',item.id,'id']]]]]]]}}" bind:change="__e" bind:__l="__l" vue-slots="{{['default']}}"><label><block wx:for="{{item.items}}" wx:for-item="son" wx:for-index="i" wx:key="i"><u-radio vue-id="{{('e4b65cbc-2-'+index+'-'+i)+','+('e4b65cbc-1-'+index)}}" name="{{son}}" bind:__l="__l" vue-slots="{{['default']}}">{{son}}</u-radio></block></label></u-radio-group></block><block wx:if="{{item.type===1}}"><u-checkbox-group vue-id="{{'e4b65cbc-3-'+index}}" shape="circle" wrap="{{true}}" active-color="#DC2672" data-event-opts="{{[['^change',[['checkboxGroupChange',['$event','$0'],[[['questionList','id',item.id,'id']]]]]]]}}" bind:change="__e" bind:__l="__l" vue-slots="{{['default']}}"><label><block wx:for="{{item.items}}" wx:for-item="son" wx:for-index="j" wx:key="j"><u-checkbox bind:input="__e" vue-id="{{('e4b65cbc-4-'+index+'-'+j)+','+('e4b65cbc-3-'+index)}}" name="{{son.name}}" value="{{son.checked}}" data-event-opts="{{[['^input',[['__set_model',['$0','checked','$event',[]],[[['questionList','id',item.id],['items','',j]]]]]]]}}" bind:__l="__l" vue-slots="{{['default']}}">{{son.name}}</u-checkbox></block></label></u-checkbox-group></block><block wx:if="{{item.type===2}}"><view class="textarea"><textarea type="text" placeholder=" " placeholderStyle="{{placeholderStyle}}" data-event-opts="{{[['input',[['textInput',['$event','$0'],[[['questionList','id',item.id,'id']]]]]]]}}" bindinput="__e"></textarea></view></block></view></block></block><u-button vue-id="e4b65cbc-5" custom-style="{{customStyle}}" type="error" shape="circle" data-event-opts="{{[['^click',[['submitAnswerOfQuestionnaire']]]]}}" bind:click="__e" bind:__l="__l" vue-slots="{{['default']}}">确认保存</u-button></view>